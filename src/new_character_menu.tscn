func delete_directory(path):
	for dir_name in OSPath.get_dirs(path):
		delete_directory(dir_name)
	for file_name in OSPath.get_files(path):
		DirAccess.remove_absolute(file_name)
	DirAccess.remove_absolute(path)
	
